DEF TRUTH_TABLE3
  PORT IN A
  PORT IN B
  PORT IN C
  PORT IN D
  PORT OUT Z

  NET NOTC
  NET NOTD

  NET I
  NET J
  NET K
  NET L

  NET M
  NET N

  INST NOTCINST NOT C NOTC
  INST NOTDINST NOT D NOTD

  INST ANDI AND A C I
  INST ANDJ AND A NOTD J
  INST ANDK AND B NOTC K
  INST ANDL AND B NOTD L

  INST ORM OR I J M
  INST ORN OR K L N

  INST OUT OR M N Z
ENDDEF